{
    "env": {
        "node": true
    },
    "extends": "/usr/local/lib/node_modules/lab/lib/linters/eslint/.eslintrc",
    "rules": {
        "no-undef": 2,
        "no-unused-expressions": 2,
        "handle-callback-err": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ]
    }
}